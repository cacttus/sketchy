{
  "name": "sketchy",
  "productName": "sketchy",
  "version": "1.0.0",
  "description": "My Electron application description.",
  "main": "./dist/Main.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --ext .tsx --ext .ts .",
    "build-dev": "./build-dev.sh",
    "start": "npm run build-dev && electron ."
  },
  "author": {
    "name": "Barney Rubble",
    "email": "b@rubble.com",
    "url": "http://barnyrubble.tumblr.com/"
  },
  "build": {
    "extends": null,
    "appId": "com.sketchy.app",
    "files": [
      "./dist/**/*",
      "./dist/Main.js"
    ],
    "asar": true,
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "./icon.png",
      "synopsis" :"Sketch app",
      "description" :"Sketch app",
      "category" :"Utility"

    },
    "win": {
      "target": "squirrel",
      "icon": "./icon.png"
    },
    "directories": {
      "buildResources": "dist"
    }
  },
  "keywords": [],
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.5",
    "@svgr/webpack": "^6.2.1",
    "@types/jquery": "^3.5.14",
    "@types/mousetrap": "^1.6.9",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-datetime-picker": "^3.4.1",
    "@types/react-dom": "^18.0.4",
    "@types/react-time-picker": "^4.0.2",
    "bootstrap-daterangepicker": "^3.1.0",
    "concurrently": "^7.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "18.2.3",
    "electron-builder": "^23.0.3",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mousetrap": "^1.6.5",
    "node-sass": "^7.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap-daterangepicker": "^7.0.0",
    "react-datetime-picker": "^3.5.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "react-svg-loader": "^3.0.3",
    "react-time-picker": "^4.5.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-keycode-enum": "^1.0.6",
    "ts-loader": "^9.3.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "bootstrap": "^5.1.3",
    "electron-build": "^0.0.3",
    "react-bootstrap": "^2.4.0",
    "rpmbuild": "^0.0.23",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
